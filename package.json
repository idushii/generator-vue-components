{
  "name": "generator-vue-components-v2",
  "displayName": "Generator vue components",
  "publisherName": "Nic34",
  "description": "Application for generating Vue file components",
  "version": "0.1.7",
  "publisher": "generator-vue-components-v2",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "activationEvents": [
    "onCommand:extension.generateComponent",
    "onCommand:extension.options"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [{
      "command": "extension.generateComponent",
      "title": "generator-vue-components: new component"
    }, {
      "command": "extension.options",
      "title": "generator-vue-components: options"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}